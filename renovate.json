{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "packageRules": [
    {
      "matchPackageNames": ["*"],
      "enabled": false
    }
  ],
  "nix": {
      "enabled": true
  },
  "lockFileMaintenance": {
    "enabled": true,
    "extends": ["schedule:daily"]
  },
  "timezone": "Asia/Tokyo",
  "dependencyDashboard": true
}
