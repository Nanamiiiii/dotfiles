{
    "include": [
        "default"
    ],
    "define": {
        "keymap": {
            "q": "set-input-mode-hiragana",
            "Q": "start-preedit",
            "x": "set-input-mode-latin",
            "X": "set-input-mode-wide-latin",
            "C-q": "set-input-mode-hankaku-katakana",
            "C-j": "commit",
            ";": "start-preedit-no-delete"
        }
    }
}
